{
  "mcpServers": {
    "thordata": {
      "command": "python",
      "args": [
        "-m",
        "thordata_mcp.main",
        "--transport",
        "stdio"
      ],
      "env": {
        "THORDATA_SCRAPER_TOKEN": "YOUR_THORDATA_SCRAPER_TOKEN",
        "THORDATA_PUBLIC_TOKEN": "YOUR_THORDATA_PUBLIC_TOKEN",
        "THORDATA_PUBLIC_KEY": "YOUR_THORDATA_PUBLIC_KEY",
        "THORDATA_BROWSER_USERNAME": "YOUR_THORDATA_BROWSER_USERNAME",
        "THORDATA_BROWSER_PASSWORD": "YOUR_THORDATA_BROWSER_PASSWORD"
      }
    }
  }
}
